{
    "manifest_version": 2,

    "name": "Mixpanel CE Visual Editor",
    "description": "This extension will inject the Mixpanel JS library into every page",
    "version": "1.1",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "content_scripts": [{
        "run_at": "document_start",
        "matches": ["<all_urls>"],
        "js": ["inject-mixpanel.js"]
    }],
    "permissions": [
       "tabs",
       "storage"
    ]
}
